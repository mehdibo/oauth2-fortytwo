{
    "name": "mehdibo/oauth2-fortytwo",
    "description": "42 Intranet OAuth 2.0 support for the PHP League's OAuth 2.0 Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehdi Bounya",
            "email": "mehdi.bounya@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "fortytwo",
        "ft",
        "42",
        "42 school",
        "1337",
        "api",
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "league/oauth2-client": "^2.0"
